{
    "name": "winter/wn-debugbar-plugin",
    "type": "winter-plugin",
    "description": "DebugBar integration for Winter CMS.",
    "homepage": "https://github.com/wintercms/wn-debugbar-plugin",
    "keywords": ["wintercms", "debugbar"],
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Bedard",
            "email": "scottbedard90@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Winter CMS Maintainers",
            "homepage": "https://wintercms.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/wintercms/wn-debugbar-plugin/issues",
        "discord": "https://discord.gg/D5MFSPH6Ux",
        "source": "https://github.com/wintercms/wn-debugbar-plugin"
    },
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0",
        "barryvdh/laravel-debugbar": "^3.15.0"
    },
    "replace": {
        "rainlab/debugbar-plugin": ">=1.0.5"
    },
    "extra": {
        "installer-name": "debugbar",
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-debugbar"
            ]
        }
    }
}
